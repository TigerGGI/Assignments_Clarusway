   "source": [
    "n = int(input(\"Enter upper limit: \"))\n",
    "prime = []\n",
    "for a in range(2, n+1):\n",
    "    count = 0\n",
    "    for b in range(2, (a // 2 + 1)):\n",
    "        if (a % b == 0):\n",
    "            count += 1\n",
    "    if (count <= 0):\n",
    "        prime.append(a)\n",
    "print(prime)"
